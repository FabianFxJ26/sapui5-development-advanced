<mvc:View controllerName="student01.com.sap.training.ux402.fullscreen.ux402fullscreen.controller.Carrier"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="idcarrierPage" title="{i18n>overviewPageTitle}">
        <content>
        <Table id="idUxCCarrierTpTable" items="{/UX_C_Carrier_TP}">
            <columns>
            <Column id="idColumn1Column" minScreenWidth="Tablet" demandPopin="true">
                <Text id="idIdColumnText" text="{i18n>columnId}"></Text>
            </Column>
            <Column id="idColumn2Column" minScreenWidth="Tablet" demandPopin="true">
                <Text id="idNameColumnText" text="{i18n>columnName}"></Text>
            </Column>
            </columns>
            <items>
            <ColumnListItem id="idcolumnListItem1ColumnListItem" press="onPress" type="Navigation">
            <cells>
            <ObjectIdentifier id="idobjectIdentifier1ObjectIdentifier" title="{Carrid}"/>
            <Text id="idCarrnameText" text="{Carrname}"></Text>
            </cells>
            </ColumnListItem>
            </items>
        </Table>
        </content>
    </Page>
</mvc:View>
